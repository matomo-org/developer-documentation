<!DOCTYPE html>
<html lang="en">
<head>
    <title>{% if title|default is not empty %}{{ title|striptags|raw }} - {% endif %}Piwik Analytics - Developer Docs - v{{ selectedPiwikVersion }}</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% if urlIfAvailableInNewerVersion %}
        <link rel="canonical" href="https://{{ constant('DOCS_DOMAIN') }}{{ urlIfAvailableInNewerVersion }}">
    {% endif %}
    <meta property="og:title" content="{% if title|default is not empty %}{{ title|striptags|raw }} - {% endif %}Piwik Analytics - Developer Docs - v{{ selectedPiwikVersion }}" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="https://{{ constant('DOCS_DOMAIN') }}{{ currentPath }}" />
    <meta property="og:image" content="https://piwik.org/wp-content/themes/piwik/assets/img/media/piwik.png" />
    <meta property="og:site_name" content="Piwik Analytics - Developer Docs" />

    <link href="/dist/css/style.min.css?{{ revision|e('html_attr') }}" rel="stylesheet">
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]--></head>
<body>
<nav class="navbar fixed-top navbar-expand-lg navbar-dark bg-dark">
    <div class="container">
        <a class="navbar-brand" href="{{ '/'|completeUrl }}">
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 926.3 244" width="74px" height="19.5">
                <path fill="#fff"
                      d="M580.7 1c-14-4-28.5 4-32.5 18l-30 105-30-105C485 8 475 .7 464 0H460.5c-11 .6-21 8-24.3 19l-30 104.7L376.6 19C372.5 5 358-3 344 1c-14 4-22 18.6-18 32.6L380.5 225c3.2 11 13 18.2 23.8 18.8h4c10.7-.6 20.5-7.8 23.7-19l30.2-105.3 30 105.4c3.3 11 13.3 18.3 24.2 18.8h3.6c11-.5 21-7.7 24-19l54.7-191.2c4-14-4-28.6-18-32.6M632.3 215c0 16 12.8 29 29 29 16 0 28.8-13 28.8-29V29c0-16.2-12.7-29-28.8-29-16 0-29 12.8-29 29v186M235.2 215c0 16.2 12.8 29 29 29 16 0 28.8-12.8 28.8-29V29c0-16.2-12.8-29-29-29-16 0-28.8 12.8-28.8 29v186M843.5 121.2l74.8-74.8C929 35.7 929 18.7 918.3 8S890.6-2.7 880 8l-89.4 89.3V29c0-16.2-12.7-29-29-29-16 0-28.8 12.8-28.8 29v186c0 16 12.8 29 29 29 16 0 28.8-13 28.8-29v-41l14.5-14.4 74 75.2c10.6 10.8 27.6 11 38.4.3 10.8-10.5 11-27.4.3-38.3l-74.2-75.5M110.2 122H57.8V52.4H109c19.3 0 35 15.5 35 34.7 0 18.8-15 34.2-33.8 34.7zm92.2-35.2c0-46.6-36.8-84.4-83-86.3V.3H29C12.7.3 0 13 0 29.3v185.4c0 16 12.8 29 29 29 16 0 28.8-13 28.8-29v-41.4h61.7v-.2c46-1.8 83-39.6 83-86.2"></path>
            </svg>

        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item{% if isHome %} active{% endif %}">
                    <a class="nav-link" href="{{ '/'|completeUrl }}">Home</a>
                </li>
                <li class="nav-item{% if activeMenu == 'Integrate' %} active{% endif %}">
                    <a class="nav-link" href="{{ '/integration'|completeUrl }}">Integrate</a>
                </li>
                <li class="nav-item{% if activeMenu == 'Develop' %} active{% endif %}">
                    <a class="nav-link" href="{{ '/develop'|completeUrl }}">Develop</a>
                </li>
                <li class="nav-item{% if activeMenu == 'API Reference' %} active{% endif %}">
                    <a class="nav-link" href="{{ '/api-reference'|completeUrl }}">API Reference</a>
                </li>
                <li class="nav-item{% if activeMenu == 'Changelog' %} active{% endif %}">
                    <a class="nav-link" href="{{ '/changelog'|completeUrl }}">Changelog</a>
                </li>
                <li class="nav-item{% if activeMenu == 'Support' %} active{% endif %}">
                    <a class="nav-link" href="{{ '/support'|completeUrl }}">Support</a>
                </li>
            </ul>
            <form class="form-inline my-2 my-lg-0">
                <input id="quick-search-typeahead" class="form-control mr-sm-2" type="text"
                       placeholder="Quick search..."
                       aria-label="Search"
                       data-action="{{ '/data/documents'|completeUrl }}"
                       autocomplete="off">
                <select title="Select your Piwik version"
                        class="piwik-version-select custom-select"
                        name="piwik">
                    {% for i in availablePiwikVersions %}

                        <option value="{{ currentPath|completeUrlForPiwikVersion(i ~ '.x') }}"
                                {% if selectedPiwikVersion == i %}selected{% endif %}
                        >Piwik {{ i }}.X
                        </option>
                    {% endfor %}
                </select>
            </form>
        </div>
    </div>
</nav>

{% block headline %}
    {% if title is defined and title and (setPageHeader is not defined or setPageHeader) %}
        <div class="container">
            <div class="page-header"><h1>{{ title }}</h1></div>
        </div>
    {% endif %}
{% endblock %}

{% if noContainer is not defined %}
<div class="container">
{% endif %}
    {% block content %}
        {{ content|raw }}
    {% endblock %}
{% if noContainer is not defined %}
</div>
{% endif %}

{% block footer %}
    <footer>
        <div class="container">
            <p class="pull-left">
                Copyright &copy; <a href="https://piwik.org">Piwik.org</a>
                |
                <a href="https://twitter.com/piwik"><i class="fa fa-twitter"></i> Twitter</a>
                |
                <a href="https://github.com/piwik"><i class="fa fa-github"></i> GitHub</a>
            </p>
            <p class="pull-right">{% if linkToEditDocument is defined and linkToEditDocument %}
            <a href="{{ linkToEditDocument }}" target="_blank">Edit on GitHub</a>
                {% endif %}</p>
            <p class="pull-right">
                <a href="mailto:developer@piwik.org?subject=Feedback%20Developer%20Guides">Give Feedback</a>{% if linkToEditDocument is defined and linkToEditDocument %}&nbsp;|&nbsp;{% endif %}
            </p>
        </div>
    </footer>
{% endblock %}

<script src="/dist/js/libraries.min.js?{{ revision|e('html_attr') }}"></script>
<script src="/dist/js/app.min.js?{{ revision|e('html_attr') }}"></script>
<script type="text/javascript">
$(document).ready(function () {
    // Disable language auto-detect on code blocks that don't have a language
    // This prevents a messy highlighting when the block doesn't contain code
    $('pre code:not([class])').addClass('no-highlight hljs');
    hljs.initHighlighting();
});
</script>

<!-- Piwik -->
<script type="text/javascript">

    var _paq = _paq || [];
    _paq.push(["setCookieDomain", "*.piwik.org"]);
    {% block piwik_customization %}{% endblock %}
    _paq.push(["setCustomVariable", 1, 'PiwikVersion', '{{ selectedPiwikVersion }}', 'page']);
    _paq.push(["trackPageView"]);
    _paq.push(["enableLinkTracking"]);

    function track() {

        (function() {
            var u=(("https:" == document.location.protocol) ? "https" : "http") + "://demo.piwik.org/";
            _paq.push(["setTrackerUrl", u+"piwik.php"]);
            _paq.push(["setSiteId", "36"]);
            _paq.push(["addTracker", 'https://demo3.piwik.org/piwik.php', '2']);

            var d=document, g=d.createElement("script"), s=d.getElementsByTagName("script")[0]; g.type="text/javascript";
            g.defer=true; g.async=true; g.src=u+"piwik.js"; s.parentNode.insertBefore(g,s);
        })();
    }

    if (window.addEventListener) { window.addEventListener("load", track, false); }
    else if (window.attachEvent) { window.attachEvent("onload",track); }
    else {
        track();
    }
</script>
<!-- End Piwik Code -->

</body>
</html>
