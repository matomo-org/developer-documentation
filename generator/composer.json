{
    "name": "piwik/pluginapi-doc-generator",
    "description": "Generates plugin API documentation",
    "keywords": ["matomo","web","analytics"],
    "homepage": "https://matomo.org",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "The Matomo Team",
            "email": "hello@matomo.org",
            "homepage": "https://matomo.org/the-piwik-team/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Piwik\\Plugins\\": "../piwik/plugins/",
            "Piwik\\": "../piwik/core/"
        },
        "psr-0": {
            "Zend_": "../piwik/libs/",
            "HTML_": "../piwik/libs/",
            "PEAR_": "../piwik/libs/",
            "Archive_": "../piwik/libs/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/piwik/Sami"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "tsteur/sami": "dev-4.x-devphp8"
    }
}
